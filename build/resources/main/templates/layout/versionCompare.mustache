<div class="versionCompare">
    <h1>{{requirement.name}}</h1>
    <div class="diff">
        <h2>Comparing versions</h2>
        <table>
            <tr>
                <th>{{versionLeft.id}} ({{versionLeft.creationTimestamp}})</th>
                <th>{{versionRight.id}} ({{versionRight.creationTimestamp}})</th>
            </tr>
            {{#diffRows}}
            <tr>
                <td><md-block>{{newLine}}</md-block></td>
                <td><md-block>{{oldLine}}</md-block></td>
            </tr>
            {{/diffRows}}
        </table>
    </div>
</div>