<html>
    <head>
        <script type="module" src="https://md-block.verou.me/md-block.js"></script>
    </head>
    <body>
        <div class="container">
            {{#error}}
            <div class="error">
                {{error}}
            </div>
            {{/error}}
            {{#info}}
            <div class="info">
                {{info}}
            </div>
            {{/info}}

            {{#showAllRequirements}}
            {{>layout/requirements}}
            {{/showAllRequirements}}

            {{#showRequirementOverview}}
            {{>layout/requirementOverview}}
            {{/showRequirementOverview}}

            {{#showNewVersionForm}}
            {{>layout/newVersionForm}}
            {{/showNewVersionForm}}

            {{#showVersionOverview}}
            {{>layout/versionOverview}}
            {{/showVersionOverview}}

            {{#showVersionCompare}}
            {{>layout/versionCompare}}
            {{/showVersionCompare}}
        </div>
    </body>
</html>

